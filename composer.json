{
    "name":        "marlinc/passbook-bundle",
    "type":        "symfony-bundle",
    "description": "iOS Passbook for Symfony",
    "keywords":    ["passbook", "ios", "pass", "mobile", "wallet", "coupons", "tickets"],
    "homepage":    "https://github.com/eymengunay/PassbookBundle",
    "license":     "MIT",
    "authors": [
        {
            "name": "Eymen Gunay",
            "email": "eymen@egunay.com"
        }
    ],
    "repositories": [{
        "type": "composer",
        "url": "https://packages.aha.biz"
    }],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "google/apiclient": "^2.4",
        "eo/passbook": "^3.0",
        "symfony/http-kernel": "^5.4",
        "symfony/dependency-injection": "^5.4",
        "symfony/config": "^5.4"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.2"
    },
    "autoload": {
        "psr-4": { "Marlinc\\PassbookBundle\\": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
